import seaborn as sns

data = sns.load_dataset('tips')
data.head()
data.corr()
data.describe()
import matplotlib.pyplot as mtp
mtp.boxplot(data['total_bill'])
import plotly.graph_objects as go
fig = go.Figure()
fig.add_trace(go.Bar(y=data["total_bill"], x=data["sex"],color=))
fig.show()
